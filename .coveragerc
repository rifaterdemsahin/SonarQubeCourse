[run]
source =
    .
relative_files = True
branch = True
omit =
    *test*.py

[paths]
source =
    .

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == "__main__":
    pass
    raise ImportError

[xml]
output = coverage.xml
relative_files = True